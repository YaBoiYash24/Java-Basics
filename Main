   public static int add() {
       {
           System.out.println(" --- --- --- QUESTION 8 --- --- --- ");
           // # 8
           int x = 6;
           int y = 3;
           int sum = (x + y);
           System.out.println(sum);
           System.out.println(" --- --- --- QUESTION 8 END --- --- ");;
           return sum;
       }
   }

