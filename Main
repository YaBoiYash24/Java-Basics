   public static boolean is_even()
   {
       System.out.println(" --- --- --- QUESTION 16 --- --- --- ");
       // # 16
       int myInt = 17;
       if (myInt == 15) {
           System.out.println(true);
           return true;
       } else {
           System.out.println(false);
           System.out.println(" --- --- --- QUESTION 16 END --- --- ");
           return false;
       }
   }

   static void absolute_value(int number)
   {
       System.out.println(" --- --- --- QUESTION 17 --- --- --- ");
       // #17
       if (number >= 0);
       System.out.println(number);
       if (number < 0);
       System.out.println(number*-1);
       System.out.println(" --- --- --- QUESTION 17 END --- --- ");
   }

   static void maximum()
   {
       System.out.println(" --- --- --- QUESTION 18 --- --- --- ");
       // # 18
       int x = 99;
       int y = 8;
       if (y > x) {
           System.out.println("Y is the bigger number");
       } else {
           System.out.println("X is the bigger number");
       }
       System.out.println(" --- --- --- QUESTION 18 END --- --- ");
   }

   static void circle_area()
   {
       System.out.println(" --- --- --- QUESTION 19 --- --- --- ");
       // # 19
       int radius = 7;
       System.out.println((3.14)*Math.pow(radius, 2));
       System.out.println(" --- --- --- QUESTION 19 END --- --- ");
   }

   static void circumference()
   {
       System.out.println(" --- --- --- QUESTION 20 --- --- --- ");
       // # 20
       int radius = 7;
       System.out.println(2*3.14*radius);
       System.out.println(" --- --- --- QUESTION 20 END --- --- ");
   }

   static void square_area()
   {
       System.out.println(" --- --- --- QUESTION 21 --- --- --- ");
       // # 21
       int sideLength = 5;
       System.out.println(Math.pow(sideLength, 2));
       System.out.println(" --- --- --- QUESTION 21 END --- --- ");
   }

   static void square_perimeter()
   {
       System.out.println(" --- --- --- QUESTION 22 --- --- --- ");
        // # 22
       int sideLength = 5;
       System.out.println(sideLength*4);
       System.out.println(" --- --- --- QUESTION 22 END --- --- ");
   }

   static void rectangle_area()
   {
       System.out.println(" --- --- --- QUESTION 23 --- --- --- ");
       // # 23
       int width = 5;
       int length = 7;
       System.out.println(width*length);
       System.out.println(" --- --- --- QUESTION 23 END --- --- ");
   }

   static void rectangle_perimeter()
   {
       System.out.println(" --- --- --- QUESTION 24 --- --- --- ");
       // # 24
       int width = 8;
       int length = 9;
       System.out.println((2 * width) + (2 * length));
       System.out.println(" --- --- --- QUESTION 24 END --- --- ");
   }

   static void sphere_area()
   {
       System.out.println(" --- --- --- QUESTION 25 --- --- --- ");
       // # 25
       int radius = 6;
       System.out.println(3.14*Math.pow(radius, 2));
       System.out.println(" --- --- --- QUESTION 25 END --- --- ");
   }

   static void sphere_volume ()
   {
       System.out.println(" --- --- --- QUESTION 26 --- --- --- ");
       // # 26
       int radius = 6;
       System.out.println((4/3)*3.14*Math.pow(radius, 3));
       System.out.println(" --- --- --- QUESTION 26 END --- --- ");
   }

